[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    build,
    dist,
    *.egg-info,
    .eggs,
    .tox,
    .venv,
    venv,
    .env,
    .pytest_cache,
    .coverage,
    htmlcov,
    .mypy_cache,
    .ruff_cache
per-file-ignores =
    __init__.py: F401
max-complexity = 10 